<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="DragonRush" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Option virtualFolders="Shaders\;" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/DragonRush" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/DragonRush" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add option="-Wall" />
			<Add directory="$(#glfw.include)" />
			<Add directory="$(#glew.include)" />
			<Add directory="$(#glm.include)" />
			<Add directory="include" />
		</Compiler>
		<Linker>
			<Add library="glew_static" />
			<Add library="glfw3" />
			<Add library="glu32" />
			<Add library="opengl32" />
			<Add library="gdi32" />
			<Add directory="$(#glfw.lib)" />
			<Add directory="$(#glew.lib)" />
		</Linker>
		<Unit filename="include/CCube.h" />
		<Unit filename="shaders/default.glsl">
			<Option virtualFolder="Shaders/" />
		</Unit>
		<Unit filename="src/CCube.cpp" />
		<Unit filename="src/main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
